package pack1;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;

import java.awt.BorderLayout;

@SuppressWarnings("serial")
public class GuiPack1 extends JFrame {

	JFrame fr;
	JPanel jp;
	JButton b0;
	JButton b1;
	JButton b2;
	JButton b3;
	JButton b4;
	JButton b5;

	public GuiPack1() {
		jp = new JPanel();
		b0 = new JButton("0");
		b1 = new JButton("1");
		b2 = new JButton("2");
		b3 = new JButton("3");
		b4 = new JButton("4");
		b5 = new JButton("5");
		fr = new JFrame("GuiPack1");
		fr.setDefaultCloseOperation(EXIT_ON_CLOSE);
		fr.setSize(500, 500);
		fr.setLocation(500, 100);
		jp.add(b0);
		jp.add(b1);
		jp.add(b2);
		jp.add(b3);
		jp.add(b4);
		jp.add(b5);
		fr.add(jp);
		setResizable(false);
		setAlwaysOnTop(true);
		fr.setVisible(true);
	}

	public JFrame getFr() {
		return fr;
	}

}
