package pack1;

import java.math.BigInteger;

public class BinKoeffizent {

	public static BigInteger calc(Long n, Long k) {

		BigInteger nf = Fakultet.fakultet(n);//n! Fakultät
		BigInteger kf = Fakultet.fakultet(k);//k! Fakultät
		BigInteger nMinusK = Fakultet.fakultet(n - k);//(n-k)! Fakultät
		BigInteger mul = nMinusK.multiply(kf);//(n-k)!/k! Fakultät
		BigInteger erg = nf.divide(mul);
	//	BigInteger erg2 = erg.divide(nMinusK);
		return erg;
	}

}
